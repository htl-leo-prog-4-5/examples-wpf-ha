<Window x:Class="Enterprise.WPF.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enterprise.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Label Content="Hallo"></Label>
        <Label Content="{Binding ServiceValue}" />
        <Button Content="Call Service" Command="{Binding CallMyService}"></Button>
        <Separator></Separator>
        <Label Content="Show AutoMapper"></Label>
        <Label Content="{Binding MyInfo.Name}" />
        <Button Content="Call Info Service" Command="{Binding CallMyInfoService}"></Button>

    </StackPanel>
</Window>
